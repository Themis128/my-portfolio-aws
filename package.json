{
  "name": "your-portfolio",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "node scripts/run-next.mjs dev -p 3003",
    "build": "node scripts/run-next.mjs build",
    "build:mcp": "tsc -p tsconfig.mcp.json",
    "start": "node scripts/run-next.mjs start",
    "start:mcp": "node build/index.js",
    "deploy": "node scripts/deploy-aws.mjs",
    "typecheck": "tsc --noEmit",
    "format": "biome format",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx,md,json,mjs}\" --cache",
    "lint": "eslint .",
    "lint:fix": "eslint --fix . && biome check --write --unsafe",
    "biome:check": "biome check",
    "biome:fix": "biome check --write --unsafe",
    "format:fix": "prettier --write \"**/*.{ts,tsx,mdx,md,json,mjs}\" --cache",
    "format:biome": "biome format",
    "check": "biome check && eslint . && pnpm typecheck && pnpm format:check",
    "fix": "biome check --write --unsafe && eslint --fix . && pnpm format:fix",
    "kaggle:install": "bash scripts/install-kaggle.sh",
    "kaggle:install:win": "powershell -NoProfile -ExecutionPolicy Bypass -File .\\scripts\\install-kaggle.ps1",
    "check-secrets": "bash scripts/check-secrets.sh",
    "check-secrets:win": "powershell -File .\\scripts\\check-secrets.ps1",
    "prepare": "husky install",
    "test": "playwright test",
    "coverage": "c8 --reporter=lcov --reporter=text npx playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:auth": "playwright test auth.spec.ts",
    "test:api": "playwright test api.spec.ts",
    "test:visual": "playwright test visual.spec.ts",
    "test:performance": "playwright test performance.spec.ts",
    "test:debug": "playwright test --debug",
    "monitor:pages": "node ./scripts/playwright-monitor.mjs",
    "monitor:pages:pm2": "pm2 start ecosystem.config.js --only playwright-monitor",
    "monitor:pages:pm2:stop": "pm2 stop ecosystem.config.js --only playwright-monitor",
    "check:figma-env": "node scripts/check-figma-env.mjs",
    "sync:figma": "node scripts/figma-sync.mjs",
    "sync:figma:generate": "pnpm run sync:figma && npx ampx generate outputs && npx ampx generate forms"
  },
