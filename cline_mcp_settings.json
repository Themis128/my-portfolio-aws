{
  "version": "1.0.0",
  "name": "Cline MCP",
  "description": "Cline MCP server configuration for company branding and development tools",
  "servers": {
    "file-server": {
      "name": "D: Drive File Server",
      "description": "MCP server for accessing files and folders on D: drive with cross-platform path resolution",
      "server_type": "stdio",
      "command": "node mcp-server/index.js",
      "working_directory": "./mcp-server",
      "enabled": true,
      "tools": [
        {
          "name": "list_files",
          "description": "List files and directories in a given path. Supports cross-platform paths (Windows D: drive paths will be resolved to appropriate Linux paths)",
          "input_schema": {
            "type": "object",
            "properties": {
              "directory": {
                "type": "string",
                "description": "The directory path to list (supports Windows D: paths which will be resolved to Linux paths)"
              }
            },
            "required": ["directory"]
          }
        },
        {
          "name": "read_file",
          "description": "Read the contents of a file. Supports cross-platform paths",
          "input_schema": {
            "type": "object",
            "properties": {
              "filePath": {
                "type": "string",
                "description": "The full path to the file to read (supports Windows D: paths which will be resolved to Linux paths)"
              }
            },
            "required": ["filePath"]
          }
        },
        {
          "name": "search_files",
          "description": "Search for files matching a pattern in a directory. Supports cross-platform paths",
          "input_schema": {
            "type": "object",
            "properties": {
              "directory": {
                "type": "string",
                "description": "The directory to search in (supports Windows D: paths which will be resolved to Linux paths)"
              },
              "pattern": {
                "type": "string",
                "description": "The file pattern to search for (e.g., '*.txt')"
              }
            },
            "required": ["directory", "pattern"]
          }
        },
        {
          "name": "extract_pdf_content",
          "description": "Extract text content from a PDF file. Supports cross-platform paths and uses pdftotext for proper PDF parsing",
          "input_schema": {
            "type": "object",
            "properties": {
              "pdfPath": {
                "type": "string",
                "description": "The full path to the PDF file to extract content from (supports Windows D: paths which will be resolved to Linux paths)"
              },
              "maxPages": {
                "type": "number",
                "description": "Maximum number of pages to extract (optional, extracts all if not specified)",
                "minimum": 1
              }
            },
            "required": ["pdfPath"]
          }
        }
      ]
    },
    "auto-favicon": {
      "name": "Auto Favicon Generator",
      "description": "Generates complete favicon sets and website assets from your logo",
      "server_type": "stdio",
      "command": "node index.js",
      "working_directory": "./mcp-servers/branding-servers/auto-favicon",
      "enabled": true,
      "tools": [
        {
          "name": "generate_favicon_set",
          "description": "Creates favicon files in multiple sizes and formats from a logo image",
          "input_schema": {
            "type": "object",
            "properties": {
              "inputImage": {
                "type": "string",
                "description": "Path to the input logo image file"
              },
              "outputDirectory": {
                "type": "string",
                "description": "Directory to save the generated favicon files"
              },
              "formats": {
                "type": "array",
                "items": {
                  "type": "string",
                  "enum": ["png", "ico"]
                },
                "description": "Formats to generate (png, ico)"
              },
              "sizes": {
                "type": "array",
                "items": {
                  "type": "number"
                },
                "description": "Sizes to generate for favicons"
              }
            },
            "required": ["inputImage", "outputDirectory"]
          }
        },
        {
          "name": "generate_website_assets",
          "description": "Creates header, footer, mobile, and theme variants from a logo",
          "input_schema": {
            "type": "object",
            "properties": {
              "logoPath": {
                "type": "string",
                "description": "Path to the input logo image file"
              },
              "outputDirectory": {
                "type": "string",
                "description": "Directory to save the generated website assets"
              },
              "variants": {
                "type": "array",
                "items": {
                  "type": "string",
                  "enum": ["header", "footer", "mobile", "dark", "light"]
                },
                "description": "Types of website assets to generate"
              }
            },
            "required": ["logoPath", "outputDirectory"]
          }
        }
      ]
    },
    "image-gen": {
      "name": "Image Generation Server",
      "description": "Generates social media banners, print assets, and marketing materials",
      "server_type": "stdio",
      "command": "node index.js",
      "working_directory": "./mcp-servers/branding-servers/image-gen",
      "enabled": true,
      "tools": [
        {
          "name": "generate_social_media_banners",
          "description": "Creates banners for LinkedIn, Twitter, Facebook, Instagram, YouTube",
          "input_schema": {
            "type": "object",
            "properties": {
              "logoPath": {
                "type": "string",
                "description": "Path to the input logo image file"
              },
              "outputDirectory": {
                "type": "string",
                "description": "Directory to save the generated social media banners"
              },
              "platforms": {
                "type": "array",
                "items": {
                  "type": "string",
                  "enum": ["linkedin", "twitter", "facebook", "instagram", "youtube"]
                },
                "description": "Social media platforms to generate banners for"
              },
              "theme": {
                "type": "string",
                "description": "Theme for the banners (e.g., 'sky-gradient')"
              }
            },
            "required": ["logoPath", "outputDirectory", "platforms"]
          }
        },
        {
          "name": "generate_print_assets",
          "description": "Creates business cards, letterhead, brochures, flyers",
          "input_schema": {
            "type": "object",
            "properties": {
              "logoPath": {
                "type": "string",
                "description": "Path to the input logo image file"
              },
              "outputDirectory": {
                "type": "string",
                "description": "Directory to save the generated print assets"
              },
              "assets": {
                "type": "array",
                "items": {
                  "type": "string",
                  "enum": ["business-card", "letterhead", "brochure", "flyer"]
                },
                "description": "Types of print assets to generate"
              },
              "format": {
                "type": "string",
                "enum": ["pdf", "png"],
                "description": "Output format for the print assets"
              }
            },
            "required": ["logoPath", "outputDirectory", "assets"]
          }
        },
        {
          "name": "generate_marketing_assets",
          "description": "Creates email headers, presentation templates, ads, thumbnails",
          "input_schema": {
            "type": "object",
            "properties": {
              "logoPath": {
                "type": "string",
                "description": "Path to the input logo image file"
              },
              "outputDirectory": {
                "type": "string",
                "description": "Directory to save the generated marketing assets"
              },
              "assets": {
                "type": "array",
                "items": {
                  "type": "string",
                  "enum": ["email-header", "presentation-template", "webinar-banner", "youtube-thumbnail"]
                },
                "description": "Types of marketing assets to generate"
              },
              "theme": {
                "type": "string",
                "description": "Theme for the marketing assets"
              }
            },
            "required": ["logoPath", "outputDirectory", "assets"]
          }
        }
      ]
    },
