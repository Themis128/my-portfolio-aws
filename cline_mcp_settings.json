    "slack": {
      "name": "Slack MCP Server",
      "description": "MCP server for Slack integration with channels, messages, and DMs",
      "server_type": "stdio",
      "command": "./slack-mcp-server",
      "args": ["mcp-server", "--transport", "stdio"],
      "working_directory": "./mcp-servers/slack-mcp-server",
      "enabled": true,
      "env": {
        "SLACK_MCP_XOXP_TOKEN": "your-slack-user-token-here",
        "SLACK_MCP_LOG_LEVEL": "info"
      },
      "tools": [
        {
          "name": "conversations_history",
          "description": "Get messages from a Slack channel or DM",
          "input_schema": {
            "type": "object",
            "properties": {
              "channel_id": {
                "type": "string",
                "description": "Channel ID or name (e.g., C1234567890 or #general)"
              },
              "limit": {
                "type": "string",
                "description": "Number of messages or time range (e.g., 50 or 1d)"
              }
            },
            "required": ["channel_id"]
          }
        },
        {
          "name": "conversations_replies",
          "description": "Get replies in a Slack thread",
          "input_schema": {
            "type": "object",
            "properties": {
              "channel_id": {
                "type": "string",
                "description": "Channel ID or name"
              },
              "thread_ts": {
                "type": "string",
                "description": "Thread timestamp"
              }
            },
            "required": ["channel_id", "thread_ts"]
          }
        },
        {
          "name": "conversations_add_message",
          "description": "Post a message to a Slack channel (requires SLACK_MCP_ADD_MESSAGE_TOOL=true)",
          "input_schema": {
            "type": "object",
            "properties": {
              "channel_id": {
                "type": "string",
                "description": "Channel ID or name"
              },
              "payload": {
                "type": "string",
                "description": "Message content"
              },
              "content_type": {
                "type": "string",
                "enum": ["text/markdown", "text/plain"],
                "description": "Content type"
              }
            },
            "required": ["channel_id", "payload"]
          }
        },
        {
          "name": "conversations_search_messages",
          "description": "Search messages in Slack",
          "input_schema": {
            "type": "object",
            "properties": {
              "search_query": {
                "type": "string",
                "description": "Search query"
              },
              "filter_in_channel": {
                "type": "string",
                "description": "Channel to search in"
              }
            }
          }
        },
        {
          "name": "channels_list",
          "description": "List Slack channels",
          "input_schema": {
            "type": "object",
            "properties": {
              "channel_types": {
                "type": "string",
                "description": "Comma-separated channel types (public_channel, private_channel, im, mpim)"
              }
            },
            "required": ["channel_types"]
          }
        }
      ]
    }
  },
